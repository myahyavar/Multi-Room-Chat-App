<Window x:Class="AppServer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppServer"
        mc:Ignorable="d"
        Title="MainWindow" Height="150" Width="400" ResizeMode="NoResize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="btnServer" Content="Start Server" HorizontalAlignment="Left" Margin="70,49,0,0" VerticalAlignment="Top" Width="210" Click="Button_Click" Height="19"/>
        <TextBlock x:Name="txtCheck" HorizontalAlignment="Left" Margin="70,83,0,0" TextWrapping="Wrap" Text="..." VerticalAlignment="Top" Height="40" Width="204"/>
    </Grid>
</Window>
